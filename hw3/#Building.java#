package cscie160.hw3;

/**
 * Elevator simulation with passenger loading/unloading and building floor limits.
 * 
 * @author Reagan Williams
 * @version 1.3 (hw3)
 * @since 2011-10-05
 */
public class Building
{
    public Building()
    {
        
    }

    public getMaxFloors()
    {
        return maxFloors;
    }

    private final int maxFloors = 7;
}